{
	"name": "custom-react-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "webpack serve --mode development --config webpack.config.js",
		"build": "NODE_ENV=production webpack --mode production --config webpack.config.js",
		"build:analyze": "ANALYZE=true webpack --mode production --config webpack.config.js",
		"type-check": "tsc --noEmit",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json,md}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss,json,md}\"",
		"lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
		"lint:check": "eslint src --ext .ts,.tsx,.js,.jsx",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@commitlint/cli": "^20.0.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
		"@swc/core": "^1.13.19",
		"@swc/helpers": "^0.5.17",
		"@tailwindcss/postcss": "^4.1.13",
		"@types/node": "^24.5.2",
		"@types/react": "19.1.0",
		"@types/react-dom": "19.1.0",
		"@typescript-eslint/eslint-plugin": "^8.44.1",
		"@typescript-eslint/parser": "^8.44.1",
		"autoprefixer": "^10.4.21",
		"clean-webpack-plugin": "^4.0.0",
		"compression-webpack-plugin": "^11.1.0",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.2",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.5",
		"html-webpack-plugin": "^5.6.4",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.3",
		"mini-css-extract-plugin": "^2.9.4",
		"postcss": "^8.5.6",
		"postcss-loader": "^8.2.0",
		"postcss-preset-env": "^10.4.0",
		"prettier": "^3.6.2",
		"react-refresh": "^0.17.0",
		"style-loader": "^4.0.0",
		"swc-loader": "^0.2.6",
		"tailwindcss": "^4.1.13",
		"terser-webpack-plugin": "^5.3.14",
		"typescript": "^5.9.2",
		"typescript-plugin-css-modules": "^5.2.0",
		"webpack": "^5.101.3",
		"webpack-bundle-analyzer": "^4.10.2",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^5.2.2",
		"webpack-merge": "^6.0.1",
		"yargs-parser": "^15.0.0"
	},
	"dependencies": {
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{css,scss,less}": [
			"prettier --write"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
